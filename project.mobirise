          "_name": "footer2",
          "_customHTML": "<section class=\"footer2\" group=\"Footers\" plugins=\"amp-main\" global=\"\" once=\"footer\">\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Logo\" name=\"showLogo\" checked=\"\">\n        <input type=\"checkbox\" title=\"Copyright\" name=\"showCopyright\" checked=\"\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background-1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#eef3f9\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked=\"\" condition=\"bg.type!='color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"bg.type!='color' &amp;&amp; overlay\">\n        <input type=\"range\" inline=\"\" title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.1\" condition=\"bg.type!='color' &amp;&amp; overlay\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n    <div class=\"container footer-container\">\n        <div class=\"mbr-row wrapper\">\n            <div mbr-if=\"showLogo\" class=\"navbar-logo\">\n                <amp-img src=\"@PROJECT_PATH@/assets/images/2-320x68.png\" layout=\"responsive\" height=\"38.25\" width=\"180\" alt=\"image\" class=\"placeholder-loader\">\n                    <div placeholder=\"\" class=\"placeholder\"></div>\n                    <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/2-320x68.png\" alt=\"\" title=\"\">\n                </amp-img>\n            </div>\n            <div class=\"copyright mbr-px-2 mbr-flex mbr-jc-c mbr-ai-c\" mbr-if=\"showCopyright\">\n                <p class=\"mbr-text mbr-fonts-style mbr-regular align-center\" mbr-theme-style=\"display-4\" data-app-selector=\".copyright .mbr-text\" data-app-placeholder=\"Type Text\">\n                    © Copyright 2020-21, разработка и наполнение сайта,&nbsp;<b><a href=\"https://antieconomy.com\" class=\"text-primary\" target=\"_blank\">Владимир Лемех</a></b></p>\n            </div>\n        </div>\n    </div>\n</section>",
          "_anchor": "footer2-i",
          "_isUserblock": true,
          "_sourceTheme": "partneramp",
          "_cid": "rZZs2vlrOL",
          "_protectedParams": [],
          "_global": true,
          "_once": "footer",
          "_params": {}
        }
      ]
    },
    "01.html": {
      "settings": {
        "title": "Научно-экспертное сообщество",
        "order": 1
      },
      "components": [
        {
          "_name": "features12",
          "_sourceTheme": "consultingamp",
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".card-wrapper, .card-wrapper .card-box, .card-wrapper .card-icon": {
              "position": "relative",
              "z-index": "2"
            },
            ".card-wrapper when (@showImg)": {
              ".card-img:before, .card-img": {
                "position": "absolute",
                "top": "0",
                "left": "0"
              },
              ".card-img:before": {
                "content": "\"\"",
                "width": "100%",
                "height": "100%",
                "z-index": "1",
                "pointer-events": "none"
              },
              "&, .card-img, .card-img img, .card-img amp-img": {
                "width": "100%",
                "height": "~\"@{imgHeight}rem\"",
                "@media (max-width: 991px)": {
                  "height": "100%"
                }
              }
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor"
            },
            ".card-wrapper when not (@transparentBg)": {
              ".mbr-overlay, .card-img:before, &:hover .card-img:before": {
                "background-color": "@cardBg",
                "opacity": "@cardOpacity"
              }
            },
            ".card-wrapper when (@showIcons), (@showNumber)": {
              ".card-icon": {
                "display": "inline-block",
                ".amp-iconfont when (@showIcons)": {
                  "font-size": "~\"@{iconsSize}rem\"",
                  "width": "~\"@{iconsSize}rem\"",
                  "color": "@iconsColor !important",
                  "& when ((@iconsSize * 16) > 40)": {
                    "@media (max-width: 767px)": {
                      "font-size": "40px",
                      "width": "40px"
                    }
                  }
                }
              },
              ".card-box when not (@fullWidth)": {
                "padding-top": "5px"
              }
            },
            ".card-wrapper when (@fullWidth)": {
              "padding-left": "60px",
              "padding-right": "60px"
            },
            ".card-wrapper when not (@transparentBg), (@showBorders), (@showCardsEffect), (@fullWidth)": {
              "@media (max-width: 991px)": {
                "padding": "15px"
              }
            },
            ".card-title when (@showLine) and (@showCardsTitle)": {
              "display": "inline-block",
              "&:after": {
                "content": "\"\"",
                "display": "block",
                "background-color": "@primaryColor",
                "height": "3px",
                "width": "40px",
                "margin": "inherit",
                "margin-top": "15px",
                "& when (@cardsAlign = '3')": {
                  "@media (max-width: 767px)": {
                    "margin-right": "auto"
                  }
                }
              }
            },
            ".card-wrapper when (@showLineEffect) and (@showLine) and (@showCardsTitle)": {
              ".card-title:after": {
                "-webkit-transition": "all .2s ease",
                "transition": "all .2s ease"
              },
              "&:hover .card-title:after": {
                "width": "100%"
              }
            },
            ".card-wrapper when (@showCardsEffect)": {
              "&, .card-icon > *, .card-title, .card-title:after, .card-img:before, &, .card-box > *, svg": {
                "-webkit-transition": "all 0.3s linear",
                "transition": "all 0.3s linear"
              },
              "&:hover when not (@showBorders) and (@transparentBg) and not (@showImg)": {
                "& when (@effect = \"1\")": {
                  "-webkit-box-shadow": "0px 0px 40px 1px rgba(0, 0, 0, 0.1)",
                  "box-shadow": "0px 0px 40px 1px rgba(0, 0, 0, 0.1)"
                },
                "& when (@effect = \"2\")": {
                  "background-color": "@successColor"
                },
                ".card-icon > *, .card-box .card-title": {
                  "color": "@primaryColor !important"
                },
                "svg": {
                  "fill": "@primaryColor !important"
                }
              },
              "& when (@showBorders), not (@transparentBg)": {
                ".card-img:before": {
                  "opacity": "1"
                },
                "&:hover": {
                  ".mbr-overlay, .card-img:before, &": {
                    "background": "@primaryColor"
                  },
                  ".card-icon .amp-iconfont, .card-icon .card-number, .card-box > *": {
                    "color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important",
                    "svg": {
                      "fill": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%)"
                    }
                  },
                  ".card-box .btn[class*=\"-outline\"], .card-box .btn[class*=\"-outline\"]:hover": {
                    "color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important",
                    "border-color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important"
                  },
                  ".card-box .card-title:after when (@showLine) and (@showCardsTitle)": {
                    "background-color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important"
                  }
                }
              }
            },
            ".mbr-row when (@showBorders) and (@showBorderTop) and not (@showPadding)": {
              "border-top": "1px solid @bordersColor"
            },
            ".mbr-row .card when not (@showPadding)": {
              "padding": "0",
              "& when (@showBorders)": {
                ".card-wrapper": {
                  "border": "none"
                },
                "@media (min-width: 768px)": {
                  "&:not(.last-child) .card-wrapper": {
                    "border-right": "1px solid @bordersColor"
                  },
                  "@media (max-width: 991px)": {
                    "&.md-pb .card-wrapper when (@cardsAmount = 4)": {
                      "border-bottom": "1px solid @bordersColor"
                    },
                    "&.md-pb:nth-child(2) .card-wrapper when (@cardsAmount = 4)": {
                      "border-right": "none"
                    }
                  }
                },
                "@media (max-width: 767px)": {
                  "&:not(.last-child) .card-wrapper": {
                    "border-right": "none",
                    "border-bottom": "1px solid @bordersColor"
                  }
                }
              }
            },
            ".mbr-text": {
              "text-align": "left"
            }
          },
          "_customHTML": "<section class=\"features12\" group=\"Features\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n            <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n            <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n            <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked=\"\">\n        \n        <header>Title</header>\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n            <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n                <option value=\"1\">Left</option>\n                <option value=\"2\" selected=\"\">Center</option>\n                <option value=\"3\">Right</option>\n            </select>\n        \n        <header>Cards</header>\n            <input type=\"range\" inline=\"\" title=\"Count\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"3\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || checkIcons\">\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showCardsTitle\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Line\" name=\"showLine\" condition=\"showCardsTitle\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Line Effect\" name=\"showLineEffect\" condition=\"showLine &amp;&amp; showCardsTitle\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showCardsSubtitle\">\n            <input type=\"checkbox\" title=\"Show Text\" name=\"showCardsText\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Buttons\" name=\"showCardsButtons\">\n            <select title=\"Align\" name=\"cardsAlign\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons || showNumber\">\n                <option value=\"1\">Left</option>\n                <option value=\"2\" selected=\"\">Center</option>\n                <option value=\"3\">Right</option>\n            </select>\n            <input type=\"checkbox\" title=\"Show Images\" name=\"showImg\" checked=\"\">\n            <input type=\"range\" inline=\"\" title=\"Images Height\" name=\"imgHeight\" min=\"18\" max=\"40\" step=\"0.1\" value=\"36\" condition=\"showImg\">\n            <input type=\"checkbox\" title=\"Show Icons\" name=\"showIcons\" checked=\"\">\n            <input type=\"range\" inline=\"\" title=\"Icons Size\" name=\"iconsSize\" min=\"0.8\" max=\"4\" step=\"0.1\" value=\"3\" condition=\"showIcons\">\n            <input type=\"color\" title=\"Icons Color\" name=\"iconsColor\" value=\"#ffffff\" condition=\"showIcons\">\n            <input type=\"checkbox\" title=\"Show Number\" name=\"showNumber\">\n            <input type=\"checkbox\" title=\"Show Cards Effect\" name=\"showCardsEffect\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons || showNumber\" checked=\"\">\n                <select title=\"Effect\" name=\"effect\" condition=\"!showBorders &amp;&amp; transparentBg &amp;&amp; (showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons || showNumber) &amp;&amp; !showImg\">\n                    <option value=\"1\">Box Shadow</option>\n                    <option value=\"2\" selected=\"\">Background</option>\n                </select>\n            <input type=\"checkbox\" title=\"Show Padding\" name=\"showPadding\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons || showNumber\">\n            <input type=\"checkbox\" title=\"Borders\" name=\"showBorders\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons || showNumber\">\n            <input type=\"checkbox\" title=\"Borders Top\" name=\"showBorderTop\" condition=\"showBorders &amp;&amp; !showPadding &amp;&amp; (showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons || showNumber)\">\n            <input type=\"color\" title=\"Color\" name=\"bordersColor\" value=\"#cccccc\" condition=\"showBorders &amp;&amp; (showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons || showNumber)\">\n            <input type=\"checkbox\" title=\"Transparent BG\" name=\"transparentBg\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons || showNumber\">\n            <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#273272\" condition=\"!transparentBg &amp;&amp; (showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons || showNumber)\">\n            <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"cardOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"!transparentBg &amp;&amp; (showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons || showNumber)\">\n        \n         <header>Background</header>\n            <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\"> \n            <input type=\"range\" inline=\"\" title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">    \n            <fieldset type=\"background\" name=\"bg\">\n                <input type=\"image\" title=\"Background Image\" value=\"\">\n                <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected=\"\">\n            </fieldset>\n            <input type=\"color\" title=\"2 Color\" name=\"twoColor\" value=\"#f4f5f7\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">  \n            <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n            <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n            <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type !== 'color') || (twoBgColor &amp;&amp; bg.type == 'color')\" mbr-class=\"{'mbr-col-md-4': width2Bg == 1 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-5': width2Bg == 2 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-6': width2Bg == 3 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-7': width2Bg == 4 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-8': width2Bg == 5 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color'}\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"title-wrap mbr-pb-4\" mbr-if=\"showTitle || showSubtitle\" mbr-class=\"{'mbr-px-2': fullWidth,'align-left': titleAlign == 1, 'align-center': titleAlign == 2, 'align-right': titleAlign == 3}\">    \n            <h2 class=\"mbr-section-title mbr-semibold mbr-fonts-style\" mbr-class=\"{'mbr-pb-1':showSubtitle}\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".title-wrap .mbr-section-title\">Научно-экспертное сообщество</h2>\n            <h3 class=\"mbr-section-subtitle mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-2\" mbr-if=\"showSubtitle\" data-app-selector=\".title-wrap .mbr-section-subtitle\">\n                Features with Images and Icons Subtitle\n            </h3>\n        </div>\n        <div class=\"mbr-row mbr-jc-c\" mbr-if=\"showCardsTitle || showIcons || showCardsSubtitle || showCardsText || showCardsButtons || showIcons || showNumber\" mbr-class=\"{'mbr-px-2': fullWidth || !showPadding, 'cardsAmount2': cardsAmount == 2 &amp;&amp; showPadding}\">\n            <div class=\"card mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-6 mbr-col-lg-3 md-pb': cardsAmount == 4,\n                             'mbr-col-md-4': cardsAmount == 3,\n                             'mbr-col-md-6': cardsAmount < 3,\n                             'mbr-col-lg-6': cardsAmount == 2,\n                             'last-child': cardsAmount == 1}\">\n                <div class=\"card-wrapper mbr-flex mbr-column mbr-jc-c\" mbr-class=\"{'mbr-p-4': ((!transparentBg || showBorders) || (showCardsEffect &amp;&amp; !showBorders &amp;&amp; transparentBg &amp;&amp; effect == 2)) &amp;&amp; !showImg, 'mbr-px-1 mbr-py-3': (showCardsEffect &amp;&amp; !showBorders &amp;&amp; transparentBg &amp;&amp; effect == 1) || showImg, 'align-center': cardsAlign == 2, 'align-right': cardsAlign == 3}\">\n                    <div class=\"card-img img-with-overlay\" mbr-if=\"showImg\">\n                        <amp-img src=\"@PROJECT_PATH@/assets/images/ect16.jpg\" layout=\"responsive\" width=\"864\" class=\"mobirise-loader\" height=\"576\" alt=\"Mobirise\">\n                            <div placeholder=\"\" class=\"placeholder\"></div>\n                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/ect16.jpg\" alt=\"Mobirise\" title=\"\">\n                        </amp-img>\n                    </div>\n                    <div class=\"mbr-overlay\" mbr-if=\"!transparentBg &amp;&amp; !showImg\"></div>\n                    <div class=\"card-icon\" mbr-if=\"showIcons || showNumber\">\n                        <span mbr-icon=\"\" class=\"amp-iconfont mobi-mbri-contact-form mobi-mbri\"></span>\n                        <h4 class=\"card-number mbr-fonts-style mbr-bold mbr-white\" mbr-theme-style=\"display-5\" data-app-selector=\".card-number\" mbr-if=\"showNumber\" mbr-class=\"{'mbr-pt-2': showIcons}\">125</h4>\n                    </div>\n                    <div class=\"card-box mbr-white\" mbr-if=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\" mbr-class=\"{'mbr-pt-3': fullWidth &amp;&amp; (showIcons || showNumber)}\">\n                        <h3 class=\"card-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-title\" mbr-class=\"{'mbr-pb-2': showCardsSubtitle, 'mbr-m-auto': showLine &amp;&amp; cardsAlign == 2, 'mbr-ml-auto': showLine &amp;&amp; cardsAlign == 3}\">ТЕМАТИЧЕСКОЕ МЕРОПРИЯТИЕ</h3>\n                        <h4 class=\"card-subtitle mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-7\" mbr-if=\"showCardsSubtitle\" data-app-selector=\".card-subtitle\">\n                            Lorem ipsum dolor sit amet</h4>\n                        <p class=\"mbr-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-7\" mbr-if=\"showCardsText\" data-app-selector=\".mbr-text\" mbr-class=\"{'mbr-pt-2': (showCardsSubtitle || showCardsTitle) &amp;&amp; !fullWidth, 'mbr-pt-3': (showCardsSubtitle || showCardsTitle) &amp;&amp; fullWidth}\">Тематическое мероприятие является способом проявления научных идей, деловых инициатив, гипотез, личного опыта и успешных практик учеными и специалистами, которые идентифицировали факторы качества труда, личные и творческие компетенции в качестве участника научно-экспертного сообщества. \n<br>\n<br>Тематические мероприятия:\n<br>-форсайт-дискуссии\n<br>- тренинги\n<br>- вебинары\n<br>- тематические конференции\n<br>- митапы\n<br>- коллективные мероприятия</p>\n                        <div class=\"card-btn mbr-section-btn\" mbr-buttons=\"\" data-toolbar=\"-mbrBtnMove\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsButtons\" mbr-class=\"{'mbr-pt-2': (showCardsSubtitle || showCardsTitle || showCardsText) &amp;&amp; !fullWidth, 'mbr-pt-4': (showCardsSubtitle || showCardsTitle || showCardsText) &amp;&amp; fullWidth}\"><a class=\"btn btn-md btn-primary-outline\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">\n                            READ MORE</a></div>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"card mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-6 mbr-col-lg-3 md-pb': cardsAmount == 4,\n                             'mbr-col-md-4': cardsAmount == 3,\n                             'mbr-col-md-6 mbr-col-lg-6 last-child': cardsAmount == 2}\" mbr-if=\"cardsAmount > 1\">\n                <div class=\"card-wrapper mbr-flex mbr-column mbr-jc-c\" mbr-class=\"{'mbr-p-4': ((!transparentBg || showBorders) || (showCardsEffect &amp;&amp; !showBorders &amp;&amp; transparentBg &amp;&amp; effect == 2)) &amp;&amp; !showImg, 'mbr-px-1 mbr-py-3': (showCardsEffect &amp;&amp; !showBorders &amp;&amp; transparentBg &amp;&amp; effect == 1) || showImg, 'align-center': cardsAlign == 2, 'align-right': cardsAlign == 3}\">\n                   <div class=\"card-img img-with-overlay\" mbr-if=\"showImg\">\n                        <amp-img src=\"@PROJECT_PATH@/assets/images/02.jpg\" layout=\"responsive\" width=\"864\" class=\"mobirise-loader\" height=\"576\" alt=\"Mobirise\">\n                            <div placeholder=\"\" class=\"placeholder\"></div>\n                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/02.jpg\" alt=\"Mobirise\" title=\"\">\n                        </amp-img>\n                    </div>\n                    <div class=\"mbr-overlay\" mbr-if=\"!transparentBg &amp;&amp; !showImg\"></div>\n                    <div class=\"card-icon\" mbr-if=\"showIcons || showNumber\">\n                        <span mbr-icon=\"\" class=\"amp-iconfont mobi-mbri-users mobi-mbri\"></span>\n                        <h4 class=\"card-number mbr-fonts-style mbr-bold mbr-white\" mbr-theme-style=\"display-5\"